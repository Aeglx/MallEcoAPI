#腾讯云短信配置
TENCENT_CLOUD_SMS_APP_ID=your_tencent_cloud_sms_app_id
TENCENT_CLOUD_SMS_APP_KEY=your_tencent_cloud_sms_app_key

#微信配置
WECHAT_APP_ID=your_wechat_app_id
WECHAT_APP_SECRET=your_wechat_app_secret
