# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_USERNAME=root
DB_PASSWORD=qwerty123
DB_NAME=malleco
DB_CHARSET=utf8mb4
DB_SYNCHRONIZE=false
DB_LOGGING=false
# 数据库连接池配置
DB_CONNECTION_LIMIT=20
DB_MAX_POOL_SIZE=50
