REDIS_LIFO=true
REDIS_MAX_RETRIES_PER_REQUEST=3

#物流配置
LOGISTICS_APP_ID=your_logistics_app_id
LOGISTICS_APP_KEY=your_logistics_app_key

# 缓存配置
CACHE_TTL=3600
CACHE_MAX_ITEMS=10000
