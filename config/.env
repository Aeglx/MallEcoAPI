#公众号配置
WECHAT_PUBLIC_ACCOUNT_ID=your_wechat_public_account_id
WECHAT_PUBLIC_ACCOUNT_SECRET=your_wechat_public_account_secret

#腾讯云配置
TENCENT_CLOUD_APP_ID=your_tencent_cloud_app_id
